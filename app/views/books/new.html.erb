<h1>New book</h1>
<% if @book.errors.any? %>
<%= @book.errors.count %>
<ul>
 <% @book.errors.full_messages.each do |messege| %>
 <li><%= messege %></li>
 <% end %>
</ul>
<% end %>
<%= form_with model: @book do |f| %>
<!--%= form_with model: @book,url:'/books',method: :post do |f| %>-->

 <!--<h4>Title</h4>-->
 <%= f.text_field :title %>

 <!--<h4>body</h4>-->
 <%= f.text_area :body %>

<%= f.submit 'Create Book' %>

<% end %>